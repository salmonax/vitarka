{
  "name": "vitarka",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo add tests please",
    "move202": "mv client/dist/index.html client/dist/200.html",
    "deploy": "npm run build && npm run move202 && surge client/dist https://vitarka.surge.sh",
    "build": "rm -rf client/dist && cross-env ENV=PRODUCTION webpack-cli",
    "analyze": "webpack --env.analyze",
    "analyze:prod": "ENV=PRODUCTION webpack --env.analyze",
    "dev": "webpack-dev-server --colors --watch --host 0.0.0.0",
    "fileserver": "http-server client/ -p 3001",
    "dev-ssl": "webpack-dev-server --colors --watch --host 0.0.0.0 --https",
    "fileserver-ssl": "http-server client/ -p 3001 --ssl",
    "start": "concurrently \"npm run dev\" \"npm run fileserver\"",
    "start-ssl": "concurrently \"npm run dev-ssl\" \"npm run fileserver-ssl\"",
    "server": "ENV=PRODUCTION PORT=3000 pm2 start ./server/bin/www"
  },
